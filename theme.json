{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
      "appearanceTools": true,
      "color": {
        "defaultPalette": false,
        "palette": [
          {
            "slug": "cu-white",
            "color": "rgb(255, 255, 255)",
            "name": "themedev-white"
          },
          {
            "slug": "cu-gray-100",
            "color": "rgb(243 244 246)",
            "name": "themedev Gray 100"
          },
          {
            "slug": "cu-gray-200",
            "color": "rgb(229 231 235)",
            "name": "themedev Gray 200"
          },
          {
            "slug": "cu-gray-300",
            "color": "rgb(209 213 219)",
            "name": "themedev Gray 300"
          },
          {
            "slug": "cu-gray-400",
            "color": "rgb(156 163 175)",
            "name": "themedev Gray 400"
          },
          {
            "slug": "cu-gray-500",
            "color": "rgb(107 114 128)",
            "name": "themedev Gray 500"
          },
          {
            "slug": "cu-gray-600",
            "color": "rgb(75 85 99)",
            "name": "themedev Gray 600"
          },
          {
            "slug": "cu-gray-700",
            "color": "rgb(55 65 81)",
            "name": "themedev Gray 700"
          },
          {
            "slug": "cu-gray-800",
            "color": "rgb(31 41 55)",
            "name": "themedev Gray 800"
          },
          {
            "slug": "cu-gray-900",
            "color": "rgb(17 24 39)",
            "name": "themedev Gray 900"
          },
          {
            "slug": "cu-primary",
            "color": "rgb(239 68 68)",
            "name": "themedev Primary"
          },
          {
            "slug": "cu-accent",
            "color": "rgb(67 56 202)",
            "name": "themedev Accent"
          },
          {
            "slug": "cu-success",
            "color": "rgb(52 211 153)",
            "name": "themedev Success"
          },
          {
            "slug": "cu-info",
            "color": "rgb(96 165 250)",
            "name": "themedev Info"
          },
          {
            "slug": "cu-warning",
            "color": "rgb(251 146 60)",
            "name": "themedev Warning"
          },
          {
            "slug": "cu-danger",
            "color": "rgb(236 72 153)",
            "name": "themedev Danger"
          }
        ],
        "background": true,
        "text": true,
        "link": true,
        "custom": true,
        "duotone": [
          {
            "slug": "cu-pink-sunset",
            "colors": ["#11245e", "#dc4379"],
            "name": "udemy Pink Sunset"
          }
        ],
        "defaultGradients": false,
        "gradients": [
          {
            "slug": "cu-summer-dog",
            "name": "themedev summer dog",
            "gradient": "linear-gradient(#a8ff78, #78ffd6)"
          }
        ]
      },
      "blocks": {
        "core/site-title": {
          "color": {
            "defaultPalette": true,
            "palette": [
              {
                "slug": "cu-title",
                "color": "#ef4444",
                "name": "themedev tittle "
              }
            ],
            "link": true,
            "custom": false,
            "gradients": [],
            "customGradient": false
          },
          "typography": {
            "fontFamilies": [
              {
                "fontFamily": "Pacifico , sans-serif",
                "slug": "cu-pacifico",
                "name": "themedev pacifico"
              }
            ]
          }
        },
        "core/cover": {
          "color": {
            "duotone": [],
            "customDuotone": false
          }
        },
        "core/preformatted": {
          "typography": {
            "fontSizes": [],
            "customFontSize": false
          }
        }
      },
      "border": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      },
      "typography": {
        "fontFamilies": [
          {
            "fontFamily": "Rubik, sans-serif",
            "slug": "ht-rubik",
            "name": "themedev Rubik"
          }
        ],
        "fontSizes": [
          { "slug": "small", "size": "0.75rem", "name": "small" },
          { "slug": "medium", "size": "1.25rem", "name": "Medium" },
          { "slug": "large", "size": "2.25rem", "name": "Large" },
          { "slug": "x-large", "size": "3rem", "name": "Extra Large" },
          { "slug": "gigantic", "size": "3.75rem", "name": "Gigantic" }
        ],
        "lineHeight": true,
        "dropCap": true,
        "fontWeight": true,
        "fontStyle": true,
        "textTransform": true,
        "letterSpacing": true,
        "textDecoration": true
      },
      "layout": {
        "contentSize": "800px",
        "wideSize": "1100px"
      },
      "spacing": {
        "margin": true,
        "padding": true,
        "units": [
          "px",
          "rem",
          "vh",
          "vw",
          "%"
        ],
        "blockGap": true
      }
    
    },
    "styles": {
      "color": {
        "text": "var(--wp--preset--color--cu-gray-700)",
        "background": "var(--wp--preset--color--cu-white)"
      },
      "elements": {
        "link": {
          "color": {
            "text": "var(--wp--preset--color--cu-primary)"
          }
        }
      },
      "blocks": {
        "core/button": {
          "color": {
            "gradient": "var(--wp--preset--gradient--cu-summer-dog)"
          }
        },
        "core/pullquote": {
          "border": {
            "width": "2px",
            "radius": "10px",
            "style": "solid",
            "color": "var(--wp--preset--color--cu-primary)"
          }
        },
        "core/site-title": {
          "typography": {
            "fontFamily": "var(--wp--preset--font--family--cu-pacifico)"
          }
        }
      },
      "typography": {
        "fontFamily": "var(--wp--preset--font--family--cu-rubik)",
        "fontSize": "1rem",
        "fontStyle": "normal",
        "fontWeight": "normal",
        "lineHeight": "inherit",
        "textDecoration": "none",
        "textTransform": "none"
      },
      "spacing": {
        "margin": {
          "top": "0px",
          "bottom": "0px",
          "left": "0px",
          "right": "0px"
        },
        "padding": {
          "top": "0px",
          "bottom": "0px",
          "left": "0px",
          "right": "0px"
        },
        "blockGap": "4rem"
      }
    }
  }
  